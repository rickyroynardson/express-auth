{
    "env": {
        "es2021": true,
        "node": true
    },
    "parser": "@typescript-eslint/parser",
    "extends": [
        "plugin:@typescript-eslint/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": [
        "**/dist/*",
        "**/node_modules/*",
        "**/tsconfig.json"
    ],
    "rules": {
        "@typescript-eslint/no-explicit-any": "off"
    }
}